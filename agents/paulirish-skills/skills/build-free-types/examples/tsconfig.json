{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    /* Native TS Execution Flags */
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true,
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,

    /* Type Checking Strategy */
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "strict": true
  }
}

